<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- Aeon MQ 8 -->
	<defaultcontrol>61</defaultcontrol>
	<controls>
		<control type="image">
			<include>backgroundspecs_single</include>
			<texture background="true">$VAR[settings_background]</texture>
		</control>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>1920</width>
			<height>1080</height>
			<texture>black.png</texture>
			<animation effect="fade" end="65" condition="true">Conditional</animation>
		</control>
		<!-- default group -->
		<control type="group">
			<include>animation_window</include>
			<control type="label" id="1">
				<left>30</left>
				<top>38</top>
				<width>1860</width>
				<height>74</height>
				<align>center</align>
				<font>font_titlesettings</font>
				<textcolor>FFF8F8F8</textcolor>
			</control>
			<control type="image">
				<left>210</left>
				<top>120</top>
				<width>1400</width>
				<height>3</height>
				<texture>grey.png</texture>
				<colordiffuse>$INFO[skin.string(colortexture)]</colordiffuse>
			</control>
			<!-- Skin Logo -->
			<control type="image">
				<posx>1647</posx>
				<posy>32</posy>
				<width>210</width>
				<height>150</height>
				<texture colordiffuse="FFFFFFFF">aeonmq8_logo.png</texture>
				<aspectratio align="center" aligny="top">keep</aspectratio>
			</control>
			<!-- Versao -->
			<control type="label">
				<left>1647</left>
				<top>135</top>
				<width>210</width>
				<height>38</height>
				<align>center</align>
				<font>font_button</font>
				<textcolor>FFF8F8F8</textcolor>
				<label>$VAR[skin_version]</label>
			</control>
			<control type="label">
				<left>1647</left>
				<top>165</top>
				<width>210</width>
				<height>38</height>
				<align>center</align>
				<font>font_plot</font>
				<textcolor>$INFO[skin.string(colorheader)]</textcolor>
				<label>by MarcosQui</label>
			</control>
			<control type="textbox" id="5">
				<left>210</left>
				<top>220</top>
				<width>1500</width>
				<height>755</height>
				<label></label>
				<font>font_plot</font>
				<align>justify</align>
				<pagecontrol>61</pagecontrol>
			</control>
			<control type="scrollbar" id="61">
				<include>ScrollBar_Vertical</include>
				<left>75</left>
				<top>220</top>
				<height>755</height>
				<onleft>61</onleft>
				<onright>61</onright>
				<ondown>61</ondown>
				<onup>61</onup>
			</control>
		</control>
	</controls>
</window>

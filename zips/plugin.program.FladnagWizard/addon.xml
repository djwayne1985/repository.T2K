<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.FladnagWizard" name="Fladnag Wizard" version="1.0.0" provider-name="fladnag">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Fladnag Wizard</summary>
    <description lang="en">Fladnag Wizard allows you to gain access to the latest Fladnag Custom Builds. Credits also goes to Pedro's.last Kindgdom Builds for his help.</description>
    <platform>all</platform>
  </extension>
</addon>